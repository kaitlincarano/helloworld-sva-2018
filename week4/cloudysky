import turtle, random, time

turtle.title ('Daydreaming')
turtle.shape ('turtle')
turtle.color ('white')
turtle.fillcolor ('white')
turtle.bgcolor ('skyblue')
turtle.pensize (2)
turtle.speed (100)
turtle.penup ()


def semiCircle (size):
	turtle.pendown ()
	for i in range (0,20):
		turtle.forward (size)
		turtle.left (9)
	turtle.left (180)

for i in range (0,5):
	semiCircle (5)


#draw a semi circle



#adjust a turn




#go back to origin

